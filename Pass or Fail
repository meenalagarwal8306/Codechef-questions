#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n,x,p,x1,x2,x3;
	    cin>>n>>x>>p;
	    x1=3*x;
	    x2=n-x;
	    x3=x1-x2;
	  
	    if(x3>=p)
	    {
	        cout<<"pass"<<endl;
	    }
	    else
	    {
	        cout<<"fail"<<endl;
	    }
	}
	return 0;
}
