#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int x,c1,c2;
	    cin>>x;
	    c1=0.1*x;
	    c2=100;
	    if(c1<c2)
	    {
	        cout<<c2<<endl;
	    }
	    else if(c1>c2)
	    {
	        cout<<c1<<endl;
	    }
	    else if(c1=c2)
	    {
	        cout<<"100"<<endl;
	    }
	}
	return 0;
}
