#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	   int n;
	   cin>>n;
	   int a[n];
	   int h;
	   int count=0;
	   cin>>h;
	    for(int i=0;i<n;i++)
	    
	   {
	       cin>>a[i];
	   }
	   for(int i=0;i<n;i++)
	   {
	       if(a[i]>h)
	       {
	           count++;
	       }
	   }
	   cout<<count<<endl;
	}
	return 0;
}
