#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int t;
	cin>>t;
	for(int i=0;i<t;i++){
	    int a,b;
	    cin>>a>>b;
	    if(a>b){
	        while(a>b){
	            b=b*2;
	        }
	    }
	    else if(a<b){
	        while(a<b){
	            a=a*2;
	        }
	    }
	    if(a==b){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
}
